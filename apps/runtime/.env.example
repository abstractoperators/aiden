# Generic .env.example with LLM API keys, eliza server config, and observability
# Works out of the box with client-direct

CACHE_STORE=database # Defaults to database. Other available cache store: redis and filesystem
SERVER_PORT=3000
OPENAI_API_KEY=
CLOUDFLARE_GW_ENABLED=false
OTEL_EXPORTER_OTLP_ENDPOINT=https://api.braintrust.dev/otel
OTEL_EXPORTER_OTLP_HEADERS="Authorization=Bearer <braintrust_api_key>, x-bt-parent=project_id:<project_id>"
